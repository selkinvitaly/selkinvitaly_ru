    "koa": "^1.2.1",
    "koa-bodyparser": "^2.2.0",
    "koa-fresh": "0.0.3",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "lru-cache": "^4.0.1",
    "node-uuid": "^1.4.7",
    "nodemailer": "^2.5.0",
    "nodemailer-html-to-text": "^2.1.0",
    "pug": "^2.0.0-beta5"
  }
